#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Backend/node_modules/.pnpm/ts-jest@29.3.4_@babel+core@_8e43fae71bfb54a6a0b8e4fe6558962e/node_modules/ts-jest/node_modules:/mnt/c/Backend/node_modules/.pnpm/ts-jest@29.3.4_@babel+core@_8e43fae71bfb54a6a0b8e4fe6558962e/node_modules:/mnt/c/Backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Backend/node_modules/.pnpm/ts-jest@29.3.4_@babel+core@_8e43fae71bfb54a6a0b8e4fe6558962e/node_modules/ts-jest/node_modules:/mnt/c/Backend/node_modules/.pnpm/ts-jest@29.3.4_@babel+core@_8e43fae71bfb54a6a0b8e4fe6558962e/node_modules:/mnt/c/Backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/ts-jest@29.3.4_@babel+core@_8e43fae71bfb54a6a0b8e4fe6558962e/node_modules/ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/ts-jest@29.3.4_@babel+core@_8e43fae71bfb54a6a0b8e4fe6558962e/node_modules/ts-jest/cli.js" "$@"
fi
